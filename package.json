{
	"name": "corelation-keystone-extension",
	"displayName": "corelation-keystone",
	"description": "Tools to help with the development in Keystone",
	"version": "0.0.1",
	"publisher": "gwersebe",
	"repository": "https://github.com/gwersebe/Corelation-Keystone-Tools",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.stripJavaSQL",
				"title": "Strip Java SQL"
			},
			{
				"command": "extension.encodeJavaSQL",
				"title": "Encode Java SQL"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run esbuild-base -- --minify",
		"esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.0.2"
	}
}
